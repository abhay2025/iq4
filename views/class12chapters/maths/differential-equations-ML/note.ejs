<!-- post the comments -->
<script>
    function cmntmsg(str) {
        $('.commentaler').removeClass('d-none')
        // $('.commentaler').append(`<span>${str}</span>`)
        $('.alermssge').text(str)
    }
    $('.cross').click(() => {
        $('.commentaler').addClass('d-none')
    })
    $('.send').click(() => {
        let txt = $('#message').val()
        let ref = $('.select span').text()
        $.ajax({
            url: '/cmnt',
            method: 'POST',
            data: {
                text: txt,
                ref
            },
            success: (r) => {
                if (r == 'notregistered') {
                    $('#exampleModal1').modal('show')
                }
                else {
                    if (($('#message').val().length) < 50) {
                        cmntmsg("Please enter more than 50letters")
                    }
                    else if (($('#message').val().length) > 500) {
                        cmntmsg("Please enter less than 500letters")
                    }


                    else {
                        cmntmsg("Your Post Will Be Live Soon")
                        $('#message').val("")
                        console.log(r);
                    }
                }
            }
        })
    })
</script>

<!-- change the exercise -->
<script>
    $('select').on('change', () => {
        // const exercise = $('select').find(":selected").text();
        const exercise = $('select').val()
        $('#loadModal').modal('show')
        $('.select span').text(`class 12 differential-equations ${exercise}`)
        // console.log($('.select span').text());
        $.ajax({
            url: `/class12differentialequations-${exercise}`,
            method: 'GET',
            success: function (r) {
                showapprovedcomments()
                $('.exercises').empty()
                $('#loadModal').modal('hide')
                $('.exercises').append(r)
            }
        })


    })
</script>
<!-- Show all approved comments -->
<script>
    // const exercise1 = $('.select span').text()
    function showapprovedcomments() {
        const exercise1 = `class 12 Integeration 8.1`
        $.ajax({
            url: '/showapprovecomment',
            method: 'POST',
            data: {
                ref: exercise1
            },
            success: function (r) {
                if ((r.length != 0)) {
                    let text;
                    let data = eval(r)
                    for (i in data) {
                        for (let j = 0; j < data[i].comments.length; j++) {
                            let ref = data[i].comments[j].ref
                            let approvedat = data[i].comments[j].approved
                            if (approvedat == 'true' && ref == exercise1) {
                                text = `<div class="comment mb-3">
        <div class="cmmnt">
            <h4 style="font-family:'Poppins',sans-serif; font-weight:200;font-size:2em;word-break:break-all;"> "${data[i].comments[j].text}"</h4>

        </div>
        <div class="cmmnt-detail mt-3 mb-3">
            <span class="name ms-4">-${data[i].name}</span>
        </div>
    </div>`
                                $('.allcmmnts').append(text)
                            }
                        }

                    }
                }
            }
        })
    }
</script>