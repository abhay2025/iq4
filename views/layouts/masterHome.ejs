    <script>
        let x = document.cookie
        // console.log(x);
        if (x == '') {
            $('#username').text("User")
        }
        else {
            let ca = document.cookie.split(';');
            let no = ca.length
            for (let i = 0; i < no; i++) {
                // console.log(ca[i].substr(0, ca[i].indexOf("=")));
                if (ca[i].substr(0, ca[i].indexOf("=")) == " name") {
                    let name = ca[i].substr(ca[i].indexOf("=") + 1)
                    $('#username').text(name)
                }
            }
        }
    </script>

    <!-- CHANGE GALLERY LOCATION AND NAV LINK-->
    <script>
        let text = window.location.pathname
        // console.log(text);
        if (text != "/") {
            $('.gal').attr("href", "/")
            $('.con').attr("href", "/")
        }
        if (text == "/") {
            $('.ss .nav-item .nav-link').removeClass('active')
            $('.hom').addClass('active')
        }
        else if (text == "/specimensol") {
            $('.ss .nav-item .nav-link').removeClass('active')
            $('.spec').addClass('active')
        }
        else {
            $('.ss .nav-item .nav-link').removeClass('active')
            $('.studymat').addClass('active')
        }

        document.querySelectorAll('.ss .nav-item .nav-link').forEach(item => {
            item.addEventListener('click', event => {
                $('.ss .nav-item .nav-link').removeClass('active')
                item.classList.add('active')
            })
        })
    </script>



